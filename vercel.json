{
    "version": 2,
    "builds": [
      {
        "src": "api/app.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "/app.py"
      },
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      }
    ],
    "public": "public"
  }